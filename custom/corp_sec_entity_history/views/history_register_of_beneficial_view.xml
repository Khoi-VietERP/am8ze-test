<odoo>
    <data>
        <record id="history_register_of_beneficial_tree" model="ir.ui.view">
            <field name="name">history.register.of.beneficial.tree</field>
            <field name="model">history.register.of.beneficial</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="entity_id"/>
                </tree>
            </field>
        </record>

        <record id="history_register_of_beneficial_form" model="ir.ui.view">
            <field name="name">history.register.of.beneficial.form</field>
            <field name="model">history.register.of.beneficial</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="entity_id"/>
                        </group>
                        <group/>
                    </group>
                    <field name="line_ids">
                        <tree editable="bottom">
                            <field name="folio_in_register"/>
                            <field name="name_of_registered"/>
                            <field name="name"/>
                            <field name="nric"/>
                            <field name="address"/>
                            <field name="date_of_birth"/>
                            <field name="nationality"/>
                            <field name="number_of_share"/>
                        </tree>
                        <form>
                            <group>
                                <field name="folio_in_register"/>
                                <field name="name_of_registered"/>
                                <field name="name"/>
                                <field name="nric"/>
                            </group>
                            <group>
                                <field name="address"/>
                                <field name="date_of_birth"/>
                                <field name="nationality"/>
                                <field name="number_of_share"/>
                            </group>
                        </form>
                    </field>
                </form>
            </field>
        </record>
    </data>
</odoo>