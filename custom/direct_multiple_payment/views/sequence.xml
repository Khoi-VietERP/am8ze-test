<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <!-- Payment sequences -->
        <record id="sequence_multiple_payment_customer_invoice" model="ir.sequence">
            <field name="name">Multiple payments customer invoices sequence</field>
            <field name="code">account.multiple.payment.customer.invoice</field>
            <field name="prefix">CUST.MULTI.IN/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        <record id="sequence_multiple_payment_customer_refund" model="ir.sequence">
            <field name="name">Multiple payments customer credit notes sequence</field>
            <field name="code">account.multiple.payment.customer.refund</field>
            <field name="prefix">CUST.MULTI.OUT/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        <record id="sequence_multiple_payment_supplier_invoice" model="ir.sequence">
            <field name="name">Multiple payments supplier invoices sequence</field>
            <field name="code">account.multiple.payment.supplier.invoice</field>
            <field name="prefix">SUPP.MULTI.OUT/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        <record id="sequence_multiple_payment_supplier_refund" model="ir.sequence">
            <field name="name">Multiple payments supplier credit notes sequence</field>
            <field name="code">account.multiple.payment.supplier.refund</field>
            <field name="prefix">SUPP.MULTI.IN/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>