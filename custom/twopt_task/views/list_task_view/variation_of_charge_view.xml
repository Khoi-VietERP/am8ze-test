<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="variation_of_charge_form" model="ir.ui.view">
            <field name="name">variation.of.charge.form</field>
            <field name="model">variation.of.charge</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group string="Entity Information">
                            <group name="entity_info">
                                <field name="uen"/>
                                <field name="charge_no_id"/>
                            </group>
                            <group>
                                <field name="entity_id" invisible="1"/>
                                <field name="entity_name"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>